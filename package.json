{
  "name": "@adonisjs/ally",
  "version": "2.0.1",
  "description": "Social authenticator for AdonisJs Framework",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "posttest": "npm run coverage",
    "test:local": "FORCE_COLOR=true node bin/index.js --local",
    "test": "nyc npm run test:local",
    "test:win": "set FORCE_COLOR=true && node bin/index.js --win",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [
    "adonis",
    "adonisjs",
    "social-auth",
    "oauth"
  ],
  "author": "adonisjs,amanvirk",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/fold": "^4.0.2",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.0.0",
    "japa": "^1.0.4",
    "japa-cli": "^1.0.1",
    "nyc": "^11.1.0",
    "semver": "^5.4.1",
    "standard": "^10.0.3"
  },
  "standard": {
    "global": [
      "use"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@adonisjs/generic-exceptions": "^1.0.0",
    "debug": "^3.0.1",
    "got": "^7.1.0",
    "lodash": "^4.17.4",
    "oauth": "^0.9.15",
    "snyk": "^1.316.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-ally.git"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-ally/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-ally#readme",
  "snyk": true
}
